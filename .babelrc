{
  "presets": [
    "es2015",
    "es2016",
    "es2017",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-export-extensions",
    "jsx-control-statements",
    ["styled-components", {
      "displayName": true,
      "ssr": true
    }]
  ],
  "env": {
    "foo": {
      "plugins": ["styled-name"]
    }
  }
}
